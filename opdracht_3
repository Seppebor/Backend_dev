<?php
	$x = 0; 
	$y = 0;
?>
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>opdracht 3</title>
</head>
<body>
	<?php while($x < 11): ?>

		<?php while ($y < 11): ?>
			<p><?php echo "$x x $y = "; echo $x * $y; ?></p>
			<?php $y++; ?>
		<?php endwhile; ?>
		<?php $x++; $y = 0 ?>
	
	<?php endwhile; ?>
</body>
</html>
